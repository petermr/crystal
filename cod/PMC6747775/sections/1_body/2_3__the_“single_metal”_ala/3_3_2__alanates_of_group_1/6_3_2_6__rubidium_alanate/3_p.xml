    <mrow>
     <mrow>
      <msub>
       <mi>RbAlH</mi>
       <mn>4</mn>
      </msub>
      <mo> </mo>
      <mo stretchy="false">(</mo>
      <mi>P</mi>
      <mi>m</mi>
      <mi>m</mi>
      <mi>a</mi>
      <mo stretchy="false">)</mo>
      <mo> </mo>
      <mover>
       <mo>→</mo>
       <mrow>
        <mn>32</mn>
        <mo> </mo>
        <mi>k</mi>
        <mi>b</mi>
        <mi>a</mi>
        <mi>r</mi>
       </mrow>
      </mover>
     </mrow>
     <mrow>
      <mo> </mo>
