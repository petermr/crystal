       <msup>
        <mrow>
         <mrow>
          <mo>(</mo>
          <mrow>
           <mfrac>
            <mrow>
             <mo>∂</mo>
             <mi mathvariant="normal">u</mi>
            </mrow>
            <mrow>
             <mo>∂</mo>
             <mi mathvariant="normal">x</mi>
            </mrow>
           </mfrac>
          </mrow>
          <mo>)</mo>
         </mrow>
        </mrow>
        <mn>2</mn>
       </msup>
       <mo>+</mo>
       <msup>
        <mrow>
         <mrow>
          <mo>(</mo>
