 </disp-formula>where 
 <inline-formula id="IEq22">
  <alternatives>
   <tex-math id="M85">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$${M}_{1}=\frac{Mc}{\hslash }$$\end{document}</tex-math>
