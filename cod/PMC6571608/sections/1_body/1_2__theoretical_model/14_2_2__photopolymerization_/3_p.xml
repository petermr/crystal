     <mo stretchy="false">(</mo>
     <mi mathvariant="bold">r</mi>
     <mo>,</mo>
     <mi>t</mi>
     <mo stretchy="false">)</mo>
     <mo>=</mo>
     <mstyle displaystyle="true">
      <munderover>
       <mo>∑</mo>
       <mrow>
        <mi>i</mi>
        <mo>=</mo>
        <mn>0</mn>
       </mrow>
       <mi>H</mi>
      </munderover>
      <mrow>
       <msub>
        <mrow>
         <mrow>
          <mrow>
           <mo>Δ</mo>
           <msubsup>
            <mi>n</mi>
            <mi>i</mi>
            <mi>m</mi>
           </msubsup>
           <mrow>
            <mo>(</mo>
            <mrow>
             <mstyle mathvariant="bold" mathsize="normal">
              <mi>r</mi>
             </mstyle>
             <mo>,</mo>
             <mi>t</mi>
            </mrow>
            <mo>)</mo>
           </mrow>
          </mrow>
          <mo>|</mo>
         </mrow>
        </mrow>
        <mi>p</mi>
       </msub>
       <mo>⋅</mo>
       <mi>cos</mi>
       <mo stretchy="false">(</mo>
       <mi>i</mi>
       <mo>⋅</mo>
       <msup>
        <mstyle mathvariant="bold" mathsize="normal">
         <mi>K</mi>
        </mstyle>
        <mi>m</mi>
       </msup>
       <mo>⋅</mo>
       <mstyle mathvariant="bold" mathsize="normal">
        <mi>r</mi>
       </mstyle>
       <mo stretchy="false">)</mo>
      </mrow>
     </mstyle>
     <mo>,</mo>
    </mrow>
   </mrow>
  </math>
 </disp-formula>
