    <td align="left">Atoms</td>
    <td align="left">
     <inline-formula id="IEq9">
      <alternatives>
       <tex-math id="M43">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$\frac {8}{3}n^{3}+6n^{2}+\frac {16}{3}n+1,~n\ge 1$\end{document}</tex-math>
       <math id="M44">
        <mfrac>
         <mrow>
          <mn>8</mn>
         </mrow>
         <mrow>
          <mn>3</mn>
         </mrow>
        </mfrac>
        <msup>
         <mrow>
          <mi>n</mi>
         </mrow>
         <mrow>
          <mn>3</mn>
         </mrow>
        </msup>
        <mo>+</mo>
        <mn>6</mn>
        <msup>
         <mrow>
          <mi>n</mi>
         </mrow>
         <mrow>
          <mn>2</mn>
         </mrow>
        </msup>
        <mo>+</mo>
        <mfrac>
         <mrow>
          <mn>16</mn>
         </mrow>
         <mrow>
          <mn>3</mn>
         </mrow>
        </mfrac>
        <mi>n</mi>
        <mo>+</mo>
        <mn>1</mn>
        <mo>,</mo>
        <mspace width="1em"/>
        <mi>n</mi>
        <mo>≥</mo>
        <mn>1</mn>
       </math>
       <inline-graphic xlink:href="11671_2019_2939_Article_IEq9.gif" xmlns:xlink="http://www.w3.org/1999/xlink"/>
      </alternatives>
     </inline-formula>
