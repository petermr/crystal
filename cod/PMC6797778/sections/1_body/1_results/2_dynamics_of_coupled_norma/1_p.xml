      </mtd>
      <mtd columnalign="left">
       <mrow>
        <mo>−</mo>
        <mspace width="-.25em"/>
        <mfrac>
         <mrow>
          <mi>∂</mi>
          <mi>V</mi>
          <mo stretchy="false">(</mo>
          <msub>
           <mrow>
            <mi>Q</mi>
           </mrow>
           <mrow>
            <mi>I</mi>
