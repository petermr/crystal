   <thead>
    <tr>
     <th align="center" style="background-color:#F2F2F2" rowspan="1" colspan="1">Type Of Material</th>
     <th align="center" style="background-color:#F2F2F2" rowspan="1" colspan="1">Advantages/Disadvantages</th>
     <th align="center" style="background-color:#F2F2F2" rowspan="1" colspan="1">Power Harvesting Capabilities</th>
