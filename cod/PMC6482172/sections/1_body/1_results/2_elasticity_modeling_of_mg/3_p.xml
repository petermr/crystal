        <mo stretchy="false">(</mo>
        <mi>n</mi>
        <mi>L</mi>
        <mo>−</mo>
        <mi>h</mi>
        <mo stretchy="false">)</mo>
        <mo>−</mo>
        <mn>2</mn>
        <msubsup>
         <mrow>
          <mi>E</mi>
         </mrow>
         <mrow>
          <mi mathvariant="italic">inf</mi>
         </mrow>
         <mrow>
          <mi>a</mi>
          <mi>b</mi>
         </mrow>
        </msubsup>
        <mo stretchy="false">(</mo>
        <mi>n</mi>
