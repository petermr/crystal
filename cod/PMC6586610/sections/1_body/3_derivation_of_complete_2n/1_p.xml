    <mn>2</mn>
    <mrow>
     <mo stretchy="true">(</mo>
     <mrow>
      <msub>
       <mrow>
        <mi>M</mi>
       </mrow>
       <mrow>
        <mn>1</mn>
       </mrow>
      </msub>
      <mfrac>
       <mi>j</mi>
       <mi>r</mi>
      </mfrac>
      <mo>+</mo>
      <mi>U</mi>
      <mo stretchy="false">(</mo>
      <mi>r</mi>
      <mo stretchy="false">)</mo>
