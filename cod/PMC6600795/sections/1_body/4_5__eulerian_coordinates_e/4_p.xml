            </mrow>
           </mfrac>
          </mrow>
         </mtd>
         <mtd>
          <mrow>
           <mfrac>
            <mrow>
             <mo>∂</mo>
             <mi mathvariant="normal">v</mi>
            </mrow>
            <mrow>
             <mo>∂</mo>
             <mi mathvariant="normal">y</mi>
            </mrow>
           </mfrac>
          </mrow>
         </mtd>
        </mtr>
       </mtable>
      </mrow>
      <mo>]</mo>
     </mrow>
     <mo>.</mo>
    </mrow>
