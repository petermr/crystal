            <mi>L</mi>
            <mi>S</mi>
            <mi>y</mi>
           </mrow>
          </msub>
          <mo stretchy="false">)</mo>
         </mrow>
         <mrow>
          <mi>∂</mi>
          <msub>
           <mrow>
            <mi>Q</mi>
           </mrow>
           <mrow>
            <mi>L</mi>
