 <disp-formula id="Equ14">
  <label>14</label>
  <alternatives>
   <tex-math id="M57">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\begin{array}{l}(-\frac{w}{c}-\frac{{\rm{\Delta }}mc}{\hslash }+i{V}_{t}(r)){{\rm{\Psi }}}_{3}(r,{\rm{\Phi }})-(-\frac{i}{r}{\partial }_{{\rm{\Phi }}}+\frac{1}{r}+{\partial }_{r}){{\rm{\Psi }}}_{4}(r,{\rm{\Phi }}){e}^{-i{\rm{\Phi }}}\\ \,-\,(\frac{i}{r}{\partial }_{{\rm{\Phi }}}+\frac{1}{r}+{\partial }_{r}){{\rm{\Psi }}}_{1}(r,{\rm{\Phi }}){e}^{i{\rm{\Phi }}}=0\end{array}$$\end{document}</tex-math>
