          <mi>y</mi>
         </mrow>
        </msub>
       </mrow>
      </mtd>
      <mtd columnalign="center">
       <mo>=</mo>
      </mtd>
      <mtd columnalign="left">
       <mrow>
        <mo>−</mo>
        <mspace width="-.25em"/>
        <mfrac>
         <mrow>
          <mi>∂</mi>
          <mi>V</mi>
          <mo stretchy="false">(</mo>
          <msub>
           <mrow>
            <mi>Q</mi>
           </mrow>
           <mrow>
            <mi>I</mi>
            <mi>R</mi>
           </mrow>
          </msub>
          <mo>,</mo>
          <msub>
           <mrow>
            <mi>Q</mi>
           </mrow>
           <mrow>
            <mi>L</mi>
            <mi>S</mi>
            <mi>x</mi>
           </mrow>
          </msub>
          <mo>,</mo>
          <msub>
           <mrow>
            <mi>Q</mi>
           </mrow>
           <mrow>
            <mi>L</mi>
            <mi>S</mi>
            <mi>y</mi>
           </mrow>
          </msub>
          <mo stretchy="false">)</mo>
         </mrow>
         <mrow>
          <mi>∂</mi>
          <msub>
           <mrow>
            <mi>Q</mi>
           </mrow>
           <mrow>
            <mi>L</mi>
            <mi>S</mi>
            <mi>y</mi>
           </mrow>
          </msub>
         </mrow>
        </mfrac>
        <mo>−</mo>
        <msub>
         <mrow>
          <mi>γ</mi>
         </mrow>
         <mrow>
          <mi>L</mi>
          <mi>S</mi>
         </mrow>
