     </mrow>
    </munderover>
    <mrow>
     <msup>
      <mrow>
       <mi>G</mi>
      </mrow>
      <mrow>
       <mtext>E</mtext>
      </mrow>
     </msup>
     <mo linebreak="badbreak">∗</mo>
     <mrow>
      <mo stretchy="true">(</mo>
      <mrow>
       <msubsup>
        <mi>ω</mi>
