      <mo>∣</mo>
      <msub>
       <mrow>
        <mi>↓</mi>
        <mo>⟩</mo>
       </mrow>
       <mrow>
        <mi>m</mi>
        <mi>n</mi>
        <mn>2</mn>
       </mrow>
      </msub>
      <mo>-</mo>
      <mo>∣</mo>
      <msub>
       <mrow>
        <mi>↓</mi>
        <mo>⟩</mo>
       </mrow>
       <mrow>
        <mi>m</mi>
        <mi>n</mi>
        <mn>1</mn>
       </mrow>
      </msub>
      <mo>∣</mo>
      <msub>
       <mrow>
        <mi>↑</mi>
        <mo>⟩</mo>
       </mrow>
       <mrow>
        <mi>m</mi>
        <mi>n</mi>
        <mn>2</mn>
       </mrow>
