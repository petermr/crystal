 <disp-formula id="Equc">
  <alternatives>
   <tex-math id="M97">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\begin{array}{rcl}{{\rm{\Psi }}}^{+}(r) &amp; = &amp; -\frac{2}{(U{(r)}^{2}-{M}_{1}^{2})}({M}_{1}\frac{j}{r}+U(r)\frac{d}{dr}){{\rm{\Psi }}}_{0}(r),\\ {{\rm{\Psi }}}^{-}(r) &amp; = &amp; -\frac{2}{(U{(r)}^{2}-{M}_{1}^{2})}(U(r)\frac{j}{r}r+{M}_{1}\frac{d}{dr}){{\rm{\Psi }}}_{0}(r)\end{array}$$\end{document}</tex-math>
