        <mn>1</mn>
       </mrow>
      </msub>
      <mo>;</mo>
      <mi>u</mi>
      <mo>,</mo>
      <mi>v</mi>
      <mo>,</mo>
      <mi>w</mi>
     </mrow>
     <mo stretchy="true">)</mo>
    </mrow>
   </mrow>
  </math>
