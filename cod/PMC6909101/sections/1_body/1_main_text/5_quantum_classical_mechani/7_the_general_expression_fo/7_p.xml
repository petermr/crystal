       <mrow>
        <mi>m</mi>
       </mrow>
       <mrow>
        <mi>ι</mi>
        <mn>2</mn>
       </mrow>
      </msub>
      <mo>,</mo>
      <msub>
       <mrow>
        <mi>m</mi>
