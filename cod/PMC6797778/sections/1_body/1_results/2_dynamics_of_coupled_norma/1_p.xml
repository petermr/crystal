        </msub>
        <mo>+</mo>
        <mi>F</mi>
        <mo stretchy="false">(</mo>
        <mi>t</mi>
        <mo stretchy="false">)</mo>
        <mo>,</mo>
        <mspace width=".25em"/>
        <mi mathvariant="normal">where</mi>
        <mspace width=".25em"/>
        <msub>
         <mrow>
          <mi>Q</mi>
         </mrow>
         <mrow>
          <mi>I</mi>
          <mi>R</mi>
         </mrow>
        </msub>
        <mo>=</mo>
        <mo stretchy="false">{</mo>
        <msub>
         <mrow>
          <mi>Q</mi>
         </mrow>
         <mrow>
          <mi>I</mi>
          <mi>R</mi>
          <mi>x</mi>
         </mrow>
        </msub>
        <mo>,</mo>
        <msub>
