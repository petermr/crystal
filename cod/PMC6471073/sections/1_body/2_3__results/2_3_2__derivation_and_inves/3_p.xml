     </msub>
     <mo stretchy="false">(</mo>
     <mi>t</mi>
     <mo stretchy="false">)</mo>
     <mo>+</mo>
     <msub>
      <mi>ϕ</mi>
      <mrow>
       <mi>θ</mi>
       <mn>2</mn>
      </mrow>
     </msub>
     <mo stretchy="false">(</mo>
     <mi>θ</mi>
     <mo stretchy="false">)</mo>
     <msub>
      <mi>q</mi>
      <mn>2</mn>
     </msub>
     <mo stretchy="false">(</mo>
