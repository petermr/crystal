          <mi>x</mi>
         </mrow>
        </msub>
        <mo>,</mo>
       </mrow>
      </mtd>
     </mtr>
     <mtr columnalign="left">
      <mtd columnalign="right">
       <mrow>
        <msub>
         <mrow>
          <mover accent="true">
           <mi>Q</mi>
