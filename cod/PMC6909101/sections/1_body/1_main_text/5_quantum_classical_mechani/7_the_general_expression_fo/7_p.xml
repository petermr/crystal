     <mrow>
      <mn>1</mn>
     </mrow>
     <mrow>
      <mn>2</mn>
      <mi>π</mi>
      <mi>i</mi>
     </mrow>
    </mfrac>
    <mrow>
     <mo stretchy="true">∮</mo>
     <mrow>
      <mfrac>
       <mrow>
        <mi>d</mi>
        <mi>u</mi>
       </mrow>
       <mrow>
        <msup>
         <mrow>
          <mi>u</mi>
         </mrow>
