                <mo>)</mo>
               </mrow>
               <mo>=</mo>
               <msub>
                <mi>A</mi>
                <mi>y</mi>
               </msub>
               <mi>sin</mi>
               <mrow>
                <mo>(</mo>
                <mrow>
                 <mn>2</mn>
                 <mi>π</mi>
                 <msub>
                  <mi>f</mi>
                  <mi>y</mi>
                 </msub>
                 <mi>t</mi>
                 <mo>−</mo>
                 <msub>
                  <mi>ϕ</mi>
                  <mi>y</mi>
                 </msub>
                </mrow>
                <mo>)</mo>
               </mrow>
              </mrow>
             </mtd>
            </mtr>
            <mtr>
             <mtd>
              <mrow>
               <msub>
                <mi>z</mi>
                <mi>v</mi>
               </msub>
               <mrow>
                <mo>(</mo>
                <mi>t</mi>
                <mo>)</mo>
               </mrow>
               <mo>=</mo>
               <msub>
                <mi>A</mi>
                <mi>z</mi>
               </msub>
               <mi>sin</mi>
               <mrow>
                <mo>(</mo>
                <mrow>
                 <mn>2</mn>
                 <mi>π</mi>
                 <msub>
                  <mi>f</mi>
                  <mi>z</mi>
                 </msub>
                 <mi>t</mi>
                 <mo>−</mo>
                 <msub>
                  <mi>ϕ</mi>
                  <mi>z</mi>
                 </msub>
                </mrow>
                <mo>)</mo>
               </mrow>
              </mrow>
             </mtd>
            </mtr>
           </mtable>
          </mrow>
         </mrow>
        </mrow>
       </mrow>
      </math>
     </inline-formula>
    </td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">
     <italic>∅
      <sub>x</sub>
     </italic>, 
     <italic>∅
