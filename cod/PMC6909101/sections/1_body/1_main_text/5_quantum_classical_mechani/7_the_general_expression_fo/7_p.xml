       <mi>L</mi>
      </mrow>
      <mo stretchy="true">)</mo>
     </mrow>
    </mrow>
    <munder>
     <mo movablelimits="false">∑</mo>
     <mrow>
      <mi>l</mi>
     </mrow>
    </munder>
    <mrow>
     <msub>
      <mrow>
       <mi>ω</mi>
      </mrow>
      <mrow>
       <mi>l</mi>
      </mrow>
     </msub>
     <msub>
      <mrow>
       <mover accent="true">
        <mi>p</mi>
